{
  "name": "@b402layer/payment-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for b402Layer Payment Infrastructure",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "b402layer",
    "payment",
    "blockchain",
    "bnb-chain",
    "cryptocurrency",
    "ai-agents",
    "web3",
    "sdk"
  ],
  "author": "b402Layer Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/b402Layer/payment-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/b402Layer/payment-sdk/issues"
  },
  "homepage": "https://github.com/b402Layer/payment-sdk#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.14.0",
    "@noble/hashes": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
